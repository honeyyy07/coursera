name <- "John"
age <- 40

name   # output "John"
age    # output 40

name <- "John Doe"

print(name) # print the value of the name variable
# numeric
x <- 10.5
class(x)

# integer
x <- 1000L
class(x)

# complex
x <- 9i + 3
class(x)

# character/string
x <- "R is exciting"
class(x)

# logical/boolean
x <- TRUE
class(x)

str <- "Hello World!"

grepl("H", str)
grepl("Hello", str)
grepl("X", str)

